{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true
  },
  "projects": {
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    },
    "clerk-contracts": {
      "type": "library",
      "root": "libs/clerk-contracts",
      "entryFile": "index",
      "sourceRoot": "libs/clerk-contracts/src",
      "compilerOptions": {
        "tsConfigPath": "libs/clerk-contracts/tsconfig.lib.json"
      }
    },
    "user-contracts": {
      "type": "library",
      "root": "libs/user-contracts",
      "entryFile": "index",
      "sourceRoot": "libs/user-contracts/src",
      "compilerOptions": {
        "tsConfigPath": "libs/user-contracts/tsconfig.lib.json"
      }
    },
    "blog-contracts": {
      "type": "library",
      "root": "libs/blog-contracts",
      "entryFile": "index",
      "sourceRoot": "libs/blog-contracts/src",
      "compilerOptions": {
        "tsConfigPath": "libs/blog-contracts/tsconfig.lib.json"
      }
    },
    "comment-contracts": {
      "type": "library",
      "root": "libs/comment-contracts",
      "entryFile": "index",
      "sourceRoot": "libs/comment-contracts/src",
      "compilerOptions": {
        "tsConfigPath": "libs/comment-contracts/tsconfig.lib.json"
      }
    }
  }
}